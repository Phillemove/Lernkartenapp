<Window x:Class="De.HsFlensburg.ClientApp101.Ui.Desktop.Statistics"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:De.HsFlensburg.ClientApp101.Ui.Desktop"
        mc:Ignorable="d"
        Title="Statistics" Height="450" Width="800">
    <Window.DataContext >
        <Binding Source = "{StaticResource VMLocator}" Path = "StatisticViewModel" ></Binding >
    </Window.DataContext >

    <Grid>
        <Label x:Name = "Statistic" Content = "Statistic" />
        <TextBlock HorizontalAlignment = "Left" Margin = "239,193,0,0" TextWrapping = "Wrap" Text = "This window is going to show some statistics" VerticalAlignment = "Top" Height = "20" Width = "250" />
    </Grid>
</Window>
